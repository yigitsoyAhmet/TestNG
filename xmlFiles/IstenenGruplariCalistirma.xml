<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Istenen gruplari calistirma">

    <test name="smoke test">

        <groups>
            <run>
                <include name="smoke"></include>
            </run>
        </groups>
        <packages>
            <package name="tests.*"></package>
        </packages>

    </test>

</suite>